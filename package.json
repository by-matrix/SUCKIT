{
  "name": "suckit",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "cd backend && node server.js",
    "server:dev": "cd backend && nodemon server.js",
    "seed": "cd backend && node seedDatabase.js",
    "backend:install": "cd backend && npm init -y && npm install express mongoose cors dotenv bcryptjs jsonwebtoken",
